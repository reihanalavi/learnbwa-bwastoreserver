<table id="example2" class="table table-bordered table-hover">
    <thead>
        <tr>
        <th>Nomor</th>
        <th>Nama</th>
        <th>Aksi</th>
        </tr>
    </thead>
    <tbody>
        <% for(let i = 0; i < category.length; i++) { %>
           <tr>
                <td><%=i + 1 %></td>
                <td><%=category[i].name %></td>
                <td>
                    <form action="/category/delete/<%= category[i]._id %>?_method=DELETE" method="post">
                        <a href="/category/edit/<%= category[i]._id %>" class="btn btn-warning">
                            <i class="nav-icon fas fa-edit"></i> Ubah</a>
                        <button href="" class="btn btn-danger">
                            <i class="nav-icon fas fa-trash"></i> Hapus</button>
                    </form>
                </td>
           </tr> 
        <% } %>
    </tbody>
    <tfoot>
    </tfoot>
</table>